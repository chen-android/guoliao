<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
	
	<FrameLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		
		<TextView
			android:id="@+id/unlocak_money_show_no_friend_tv"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="center"
			android:text="@string/No_friends"
			android:textColor="@color/group_list_gray"
			android:visibility="gone"/>
		
		
		<ListView
			android:id="@+id/unlocak_money_friendlistview_iv"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_gravity="center"
			android:divider="@null"/>
		
		<TextView
			android:id="@+id/unlocak_money_dialog"
			android:layout_width="80.0dip"
			android:layout_height="80.0dip"
			android:layout_gravity="center"
			android:background="@drawable/rc_bg_toast"
			android:gravity="center"
			android:textColor="#ffffffff"
			android:textSize="30.0dip"
			android:visibility="invisible"/>
		
		<com.GuoGuo.JuicyChat.server.pinyin.SideBar
			android:id="@+id/unlocak_money_sidrbar_sb"
			android:layout_width="30.0dip"
			android:layout_height="fill_parent"
			android:layout_gravity="right|center"/>
	
	</FrameLayout>
</LinearLayout>